<%
  breadcrumb = '';
  title = '';
  subTitle = '';
  IMG = '/assets/img/';
  RESOURCE = '/assets/';
%>
<!DOCTYPE html>
<html lang="ja">
<%- include('../include/_head', {meta: json.index}); %>
<body>
  <%- include('../include/_header'); %>
  <div class="l-mainvisual l-mainvisual-under">
    <div class="l-mainvisual-inner">
      <%- include('../include/_mv', { title: 'COMPANY', subTitle: '私たちについて' } ); %>
    </div>
  </div><!-- ./l-mainvisual -->
  <%- include('../include/_breadcrumb', { breadcrumb: 'COMPANY' } ); %>
  <main id="main" class="l-main l-main-under">
    <div class="l-main-inner">
      <div class="l-company l-top-company">
        <section class="l-section" data-inner-space="none">
          <div class="l-section-inner">
            <%- include('../include/company/_company'); %>
          </div><!-- ./l-section-inner -->
        </section>
        <section class="l-section">
          <div class="l-section-inner">
            <%- include('../include/company/_company_detail'); %>
          </div><!-- ./l-section-inner -->
        </section>
      </div>
      <section class="l-section" data-inner-space="none" data-inner-width="full">
        <div class="l-section-inner">
          <%- include('../include/_contact'); %>
        </div><!-- ./l-section-inner -->
      </section>
    </div><!-- ./l-main-inner -->
  </main>
  <%- include('../include/_footer'); %>
  <script src="https://unpkg.com/swiper@8/swiper-bundle.min.js"></script>
  <script src="<%- RESOURCE %>/js/index.js"></script>
</body>
</html>